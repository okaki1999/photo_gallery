<%= form_with(model: @photo, url: photos_path, local: true, html: { multipart: true }) do |form| %>

  <div class="field">
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>

  <div class="field">
    <%= form.label :image_url %>
    <%= form.file_field :image_url %>
  </div>

  <div class="actions">
    <%= form.submit 'Upload' %>
  </div>
<% end %>